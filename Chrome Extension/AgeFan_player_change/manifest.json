{
    "manifest_version": 2,
  
    "name": "Age Player Plugin",
    "description": "This extension change the player for agefan",
    "version": "1.0",
  
    "browser_action": {
     "default_icon": "icon.png",
     "default_popup": "popup.html"
    },
    "content_scripts": [
        {
          "run_at": "document_end",
          "matches": ["https://www.agefans.vip/play/*","https://www.agefans.live/play/*"],
          "js": ["background.js"]
        }
      ]
    ,
    "permissions": [
     "activeTab"
     ]
  }